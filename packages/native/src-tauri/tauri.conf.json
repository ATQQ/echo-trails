{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "记忆的回响",
  "version": "0.1.2",
  "identifier": "com.echo-trails.app",
  "build": {
    "beforeDevCommand": "cd ../app && VITE_BASE_ORIGIN=http://10.109.1.243:1420 TAURI=true bun run dev",
    "devUrl": "http://10.109.1.243:1420",
    "beforeBuildCommand": "cd ../app && VITE_BASE_ORIGIN=https://photo.sugarat.top TAURI=true bun run build",
    "frontendDist": "../../app/dist"
  },
  "app": {
    "windows": [
      {
        "title": "echo-trails",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/192x192.png",
      "icons/icon.ico"
    ]
  }
